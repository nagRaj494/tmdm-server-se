            <xsd:appinfo source="X_Write">DS_Manager_MWest</xsd:appinfo>  
            <xsd:appinfo source="X_Label_EN">Photo</xsd:appinfo>  
            <xsd:appinfo source="X_Label_FR">Photo</xsd:appinfo> 
          </xsd:annotation> 
        </xsd:element>  
