<xsd:appinfo source="X_ForeignKeyInfo">Region/Region</xsd:appinfo>
<xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
</xsd:annotation>
</xsd:element>
            </xsd:sequence> 
    </xsd:complexType>
        <xsd:unique name="Employee">
<xsd:selector xpath="."/>
<xsd:field xpath="EEID"/>
</xsd:unique>
