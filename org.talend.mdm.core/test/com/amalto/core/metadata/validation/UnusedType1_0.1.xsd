<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://www.w3.org/2001/XMLSchema"/>
    <xsd:element name="JobGroup" type="JobGroupType">
        <xsd:annotation>
            <xsd:appinfo source="X_Write">General_Manager</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="JobGroup">
            <xsd:selector xpath="."/>
            <xsd:field xpath="JobGroup"/>
        </xsd:unique>
    </xsd:element>
    <xsd:complexType name="JobGroupType">
        <xsd:all maxOccurs="1" minOccurs="1">
            <xsd:element maxOccurs="1" minOccurs="1" name="JobGroup" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">General_Manager</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="IsComposedOf" type="IsLinkedTo">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">General_Manager</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="IsLinkedTo">
        <xsd:sequence maxOccurs="unbounded" minOccurs="0">
            <xsd:element maxOccurs="1" minOccurs="1" name="JobCategory" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">General_Manager</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="IsComposedOf">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="JobCategory" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">General_Manager</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
