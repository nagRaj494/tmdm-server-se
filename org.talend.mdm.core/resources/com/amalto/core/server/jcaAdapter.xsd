<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="JCAAdapter">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="jndiname" type="xsd:string"/>
                <xsd:element name="parameters" type="xsd:hexBinary"/>
            </xsd:sequence>
        </xsd:complexType>
        <xsd:unique name="JCAAdapter">
            <xsd:selector xpath="."/>
            <xsd:field xpath="jndiname"/>
        </xsd:unique>
    </xsd:element>
</xsd:schema>