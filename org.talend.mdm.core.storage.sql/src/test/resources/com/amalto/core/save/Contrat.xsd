  <!-- Objet EDA -->  
  <xsd:element name="Eda" type="EdaType"> 
    <xsd:annotation> 
      <xsd:appinfo source="X_Label_FR">EDA</xsd:appinfo>  
      <xsd:appinfo source="X_PrimaryKeyInfo">Eda/codeEda</xsd:appinfo>  
      <xsd:appinfo source="X_PrimaryKeyInfo">Eda/nomEda</xsd:appinfo>
      <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>  
      <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_ReadOnly</xsd:appinfo>  
        
      <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>  
        
      <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo>  
      <xsd:appinfo source="X_Schematron">&lt;pattern name="CodeEdaValidation" &gt; &lt;rule context="/"&gt;&lt;assert test="fn:not(fn:exists(controleCodeEda)) or (( controleCodeEda=&amp;quot;Oui&amp;quot;) and (string-length(codeEda) &gt;=6 and string-length(codeEda)&amp;lt;=8 and (fn:compare(substring(codeEda,6,1),&amp;quot;T&amp;quot;)=0 or fn:compare(substring(codeEda,6,1),&amp;quot;H&amp;quot;)=0) and ( (fn:compare( substring(codeEda,7,2) ,concat(&amp;quot;C&amp;quot;,substring(useDonneurdOrdre,2,1)))=0) or ((fn:compare(&amp;quot; &amp;quot;,substring(codeEda,7,1))=0) and fn:matches(substring(codeEda,8,1),&amp;quot;[1-9]&amp;quot;))or (fn:compare( substring(codeEda,7,2) ,concat(&amp;quot;C&amp;quot;,substring(frontiereEda,2,1)))=0) or (string-length(substring(codeEda,7,2))=0) ))) or (controleCodeEda=&amp;quot;Non&amp;quot;) "&gt;&lt;![CDATA[[FR: Erreur sur le Code de l'EDA ]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>  
      <xsd:appinfo source="X_Schematron">&lt;pattern name="DateDebutApplication" &gt; &lt;rule context="/"&gt;&lt;assert test="translate(//dateDebutApplication,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateDebutEda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate(//dateDebutApplication, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )&gt;number( translate(dateDebutEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) or number( translate(//dateDebutApplication, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )=number( translate(dateDebutEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de début d'application doit être supérieure ou égale à la date de début de validation]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>  
      <xsd:appinfo source="X_Schematron">&lt;pattern name="DateFinApplication" &gt; &lt;rule context="/"&gt;&lt;assert test="translate(//dateFinApplication,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinEda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate(//dateFinApplication, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )&amp;lt;number( translate(dateFinEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )or number( translate(//dateFinApplication, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )=number( translate(dateFinEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de fin d'application doit être inférieure ou égale à la date de fin de validation]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>  
      <xsd:appinfo source="X_Schematron">&lt;pattern name="DateFinValidation" &gt; &lt;rule context="/"&gt;&lt;assert test="translate(dateDebutEda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinEda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebutEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFinEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de début de validité doit être inférieure à la date de fin de validité.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="/"&gt;&lt;assert test="translate(dateDebutRREda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinRREda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebutRREda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFinRREda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de début de RR doit être inférieure à la date de fin RR.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="/"&gt;&lt;assert test="translate(dateDebutRCEda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or translate(dateFinRCEda,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number( translate( dateDebutRCEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number( translate( dateFinRCEda, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR:La date de début de RC doit être inférieure à la date de fin RC.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo> 
    </xsd:annotation>  
    <xsd:unique name="Eda"> 
      <xsd:selector xpath="."/>  
      <xsd:field xpath="idEda"/> 
    </xsd:unique> 
  </xsd:element>  
