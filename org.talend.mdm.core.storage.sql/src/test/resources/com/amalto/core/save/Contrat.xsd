      <xsd:appinfo source="X_Schematron">&lt;pattern name="DateFinValidation" &gt; &lt;rule context="dateFinContrat"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../dateDebutEffectiveContrat,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) "&gt;&lt;![CDATA[[FR:La date de fin du contrat doit être supérieure à la date de début effective du contrat.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="detailContrat/Caracteristiques/DateFinreservesRapidesEtComplementaires"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../dateDebutreservesRapidesEtComplementaires,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La date de début RR-RC doit être inferieure à la date de fin RR-RC.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="detailContrat/Caracteristiques/dateFinSsy"&gt;&lt;assert test="translate(.,&amp;quot;&amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;= number(translate(../dateDebutSsy,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La date de début Ssy doit être inferieure à la date de fin Ssy.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>  
      <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_ReadOnly</xsd:appinfo>  
      <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
      <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo> 
    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
<xsd:appinfo source="X_Write">User</xsd:appinfo>
</xsd:annotation>   
