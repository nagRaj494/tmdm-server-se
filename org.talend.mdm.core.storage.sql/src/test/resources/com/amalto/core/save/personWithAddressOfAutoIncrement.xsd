                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="fundationDate" type="xsd:date">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Fundation date</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Date de fondation</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="liquidationDate" type="xsd:date">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Liquidation date</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Date de liquidation</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="liquidationReason" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Liquidation reason</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Raison de liquidation</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="board_Roles" type="boardRoles">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Board roles</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Role et signataires</xsd:appinfo>
                        <xsd:appinfo source="X_AutoExpand">false</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="category" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_ForeignKey_NotSep">true</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKey">REF_Category/REF_CategoryId</xsd:appinfo>
                        <xsd:appinfo source="X_Description_EN">Organisation's Category</xsd:appinfo>
                        <xsd:appinfo source="X_Description_FR">Catégorie de l'organisation, utilise principalement pour les organisations paraétatiques</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Category</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Catégorie</xsd:appinfo>
