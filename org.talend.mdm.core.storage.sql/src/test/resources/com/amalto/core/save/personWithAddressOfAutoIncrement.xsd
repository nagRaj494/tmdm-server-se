                        <xsd:appinfo source="X_ForeignKeyInfo">REF_Sex/label</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Sex</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Genre</xsd:appinfo>
