        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="personId" type="AUTO_INCREMENT">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Visible_Rule">fn:false()</xsd:appinfo>
