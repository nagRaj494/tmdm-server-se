                        <xsd:appinfo source="X_ForeignKeyInfo">REF_MasterStatus/REF_MasterStatusId</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">REF_MasterStatus/label</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Entity status</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Status du tiers</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_PERS_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="firstName" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">First name</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Prénom</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_PERS_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="lastName" type="xsd:string">
