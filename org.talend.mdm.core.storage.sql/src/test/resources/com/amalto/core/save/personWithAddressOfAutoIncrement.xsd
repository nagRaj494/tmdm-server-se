                        <xsd:appinfo source="X_Label_FR">Contacts</xsd:appinfo>
                        <xsd:appinfo source="X_AutoExpand">false</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="preferedContactOrder" type="preferedContactOrderTypeOrganisation">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Description_EN">perfered contact order</xsd:appinfo>
                        <xsd:appinfo source="X_Description_FR">ordre de préférence de contact</xsd:appinfo>
