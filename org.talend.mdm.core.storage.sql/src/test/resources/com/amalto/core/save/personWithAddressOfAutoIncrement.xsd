                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="active" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Default_Value_Rule">fn:true()</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Active</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Actif</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
