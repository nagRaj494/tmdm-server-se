                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="identifiers" type="SSP_EMS_identifiersType">
                    <xsd:annotation>
                        <xsd:appinfo source="X_AutoExpand">true</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Identifiers</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Identifiants</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="organisationId" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_ForeignKey_NotSep">true</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKey">organisation/organisationId</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Organisation in relation</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Organisation en relation</xsd:appinfo>
                        <xsd:appinfo source="X_Description_EN">related organisation</xsd:appinfo>
                        <xsd:appinfo source="X_Description_FR">organisation en relation</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">organisation/idReglis</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">organisation/commonName</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="name" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Name</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Nom de l'établissement</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="alternativeName" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Alternative name</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Nom (secondaire)</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="category" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Category</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Categorie</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="address" type="address">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">Address</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Adresse</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
