                        <xsd:appinfo source="X_ForeignKeyInfo">REF_Category/REF_CategoryId</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">REF_Category/label</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="active" type="xsd:boolean">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Default_Value_Rule">fn:true()</xsd:appinfo>
