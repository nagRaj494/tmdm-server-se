                        <xsd:appinfo source="X_Label_FR">Identifiants</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_PERS_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="commonName" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Description_EN">calculated field used for search purpose</xsd:appinfo>
                        <xsd:appinfo source="X_Description_FR">champs calculé utilisé a fin de recherche</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Common name</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Nom courant</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_PERS_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="status" type="xsd:string">
