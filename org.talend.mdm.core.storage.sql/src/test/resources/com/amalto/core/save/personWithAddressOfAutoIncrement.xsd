                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" name="status" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Description_FR">statut de l'enregistrement</xsd:appinfo>
