            <xsd:appinfo source="X_Write">HEALTH_INSTITUTION</xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="1" minOccurs="1" name="emsId" type="AUTO_INCREMENT">
                    <xsd:annotation>
