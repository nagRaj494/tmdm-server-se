                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="organisationAdditionnalName" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Description_FR">nom alternatif ou commun de l'organisation</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="legalForm" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_ForeignKey">REF_LegalForm/REF_LegalFormId</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKey_NotSep">true</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">REF_LegalForm/label</xsd:appinfo>
