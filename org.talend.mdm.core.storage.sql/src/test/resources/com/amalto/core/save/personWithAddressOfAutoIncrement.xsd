                        <xsd:appinfo source="X_Visible_Rule">fn:false()</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="idReglis" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Label_EN">ID REGLIS</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">ID REGLIS</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="identifiers" type="identifiersType">
                    <xsd:annotation>
                        <xsd:appinfo source="X_AutoExpand">true</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Identifiers</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Identifiants</xsd:appinfo>
