                        <xsd:appinfo source="X_Label_FR">Forme légale</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="branchOf" type="xsd:string">
                    <xsd:annotation>
                        <xsd:appinfo source="X_Description_EN">branch of parent organisation</xsd:appinfo>
                        <xsd:appinfo source="X_Description_FR">filiale de l'organisation parente</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKey">organisation/organisationId</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKey_NotSep">true</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">organisation/idReglis</xsd:appinfo>
                        <xsd:appinfo source="X_ForeignKeyInfo">organisation/commonName</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Branch of</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Filliale de</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="locations" type="addresses">
                    <xsd:annotation>
                        <xsd:appinfo source="X_AutoExpand">false</xsd:appinfo>
                        <xsd:appinfo source="X_Label_EN">Locations</xsd:appinfo>
                        <xsd:appinfo source="X_Label_FR">Etablissements</xsd:appinfo>
                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                        <xsd:appinfo source="X_Write">X_ORG_WRITE</xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="contacts" type="contacts">
